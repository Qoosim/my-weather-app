export const myCity = () => {
    let dataReady = 0;
    let weatherObject = {};

    const getTimeStamp = () => timeStamp;
    const setDataReady = () => { dataReady = 1; };
    const clearDataReady = () => { dataReady = 0; };
    const getDataReady = () => dataReady;
    const updateWeatherObject = (obj) => {
        weatherObject = obj;
        dataReady = 1;
    };
    const getWeatherObject = () => weatherObject;


    return {
        getTimeStamp,
        setDataReady,
        clearDataReady,
        getDataReady,
        updateWeatherObject,
        getWeatherObject,
    };
};

export default myCity;