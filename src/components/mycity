export const myCity = () => {
    let dataReady = 0;
    let weatherObject = {};

    const getTimeStamp = () => timeStamp;
    const setDataReady = () => { dataReady = 1; };
    const clearDataReady = () => { dataReady = 0; };
    const updateWeatherObject = (obj) => { weatherObject = obj; };



    return {
        getTimeStamp,
        setDataReady,
        clearDataReady,
        updateWeatherObject,
    };
};

export default myCity;